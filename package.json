{
  "name": "caju-front-test-1",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite &",
    "init:db": "json-server --watch db.json &",
    "build": "tsc && vite build",
    "test:dev": "jest --watch --verbose --no-cache",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview &",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "cypress:cli": "yarn build && yarn init:db && yarn preview",
    "cypress": "yarn cypress:cli && cypress run",
    "cypress:open": "yarn init:db && yarn dev && cypress open --e2e",
    "cypress:ci:db": "json-server db.json &",
    "cypress:ci:server": "yarn preview &",
    "cypress:ci": "yarn cypress:ci:db && yarn cypress:ci:server && yarn cypress run",
    "prepare": "husky"
  },
  "dependencies": {
    "axios": "^1.7.2",
    "json-server": "^1.0.0-beta.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.53.1",
    "react-icons": "^5.2.1",
    "react-router-dom": "5.1.2",
    "styled-components": "^6.1.11"
  },
  "devDependencies": {
    "@nabla/vite-plugin-eslint": "^2.0.4",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.2",
    "@testing-library/react": "^16.0.1",
    "@testing-library/react-hooks": "^8.0.1",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/cypress": "^1.1.6",
    "@types/jest": "^29.5.14",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react": "^4.2.1",
    "cypress": "^13.15.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.34.2",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.6",
    "husky": "^9.1.6",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "prettier": "^3.3.3",
    "ts-jest": "29.1.2",
    "ts-node": "10.9.2",
    "typescript": "^5.2.2",
    "vite": "^5.2.0"
  }
}
